<!-- 分页文件 -->

<ul class="pager">
    <% if (page.prev){ %>
  <li class="previous"><a href="<%- config.root %><%- page.prev_link %>">&laquo;  <%- __('Paginator.Prev')%></a></li>
    <% } %>
    <% if (page.next){ %>
  <li class="next"><a href="<%- config.root %><%- page.next_link %>"><%- __('Paginator.Next')%>  &raquo;</a></li>
    <% } %>
</ul>